<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXListView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="850.0" prefWidth="1400.0" styleClass="home-anchor" stylesheets="@custom.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ark.optimal.wallet.ui.main.FXMLHomeViewController">
   <children>
      <AnchorPane id="AnchorPane" layoutX="318.0" layoutY="156.0" prefHeight="80.0" prefWidth="713.0" style="-fx-background-radius: 10; -fx-background-color: white;">
         <children>
            <Label fx:id="myaccountstitle" prefHeight="52.0" prefWidth="713.0" style="-fx-alignment: LEFT_CENTER; -fx-background-radius: 5;" styleClass="toolbar" stylesheets="@main/custom.css" text="My Accounts" textAlignment="CENTER" textFill="#fffdfd">
               <font>
                  <Font name="System Bold" size="22.0" />
               </font>
               <padding>
                  <Insets left="10.0" />
               </padding>
            </Label>
            <VBox layoutY="50.0" prefHeight="50.0" prefWidth="713.0">
               <children>
                  <JFXListView fx:id="homeAccounts" onMouseClicked="#handleUserAccountMouseClick" prefHeight="23.0" prefWidth="713.0" />
                  <HBox prefHeight="50.0" prefWidth="713.0">
                     <children>
                        <JFXButton fx:id="btnCreateAccount" alignment="BASELINE_LEFT" onAction="#onCreateAccount" prefHeight="42.0" prefWidth="219.0" style="-fx-text-align: center;" text="Create Account" textFill="#757575" textOverrun="CLIP">
                           <padding>
                              <Insets left="30.0" right="5.0" />
                           </padding>
                           <tooltip>
                              <Tooltip fx:id="delegateAddressTooltip1" text="Empty Tooltip" />
                           </tooltip>
                           <font>
                              <Font size="15.0" />
                           </font>
                           <graphic>
                              <ImageView fitHeight="26.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../../../../resources/create-account.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </JFXButton>
                        <JFXButton fx:id="btnImportAccount" alignment="BASELINE_LEFT" onAction="#onImportAccount" prefHeight="42.0" prefWidth="226.0" style="-fx-text-align: left;" text="Import Account" textFill="#757575" textOverrun="CLIP">
                           <padding>
                              <Insets left="30.0" right="5.0" />
                           </padding>
                           <tooltip>
                              <Tooltip fx:id="delegateAddressTooltip" text="Empty Tooltip" />
                           </tooltip>
                           <font>
                              <Font size="15.0" />
                           </font>
                           <graphic>
                              <ImageView fitHeight="23.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../../../../resources/import-account.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </JFXButton>
                        <JFXButton fx:id="btnWatchAccount" alignment="BASELINE_LEFT" onAction="#onWatchAccount" prefHeight="42.0" prefWidth="201.0" style="-fx-text-align: left;" text="Watch Account" textFill="#757575" textOverrun="CLIP">
                           <padding>
                              <Insets left="30.0" right="5.0" />
                           </padding>
                           <tooltip>
                              <Tooltip fx:id="delegateAddressTooltip2" text="Empty Tooltip" />
                           </tooltip>
                           <font>
                              <Font size="15.0" />
                           </font>
                           <graphic>
                              <ImageView fitHeight="27.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../../../../../resources/watch-account.png" />
                                 </image>
                              </ImageView>
                           </graphic>
                        </JFXButton>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
